{
    "name": "radioafricagroup/laravelsurveyjs",
    "description": "Laravel wrapper for surveyjs library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tiras Muturi",
            "email": "tirasmuturi@gmail.com",
            "homepage": "https://github.com/tirasmuturi"
        }
    ],
    "homepage": "https://github.com/radioafricagroup/laravel-surveyjs",
    "keywords": [
        "Laravel", "LaravelSurveyJs", "surveyjs", "survey manager", "laravel survey", "survey editor", "surveyjs builder", "radioafricagroup", "tirasmuturi"
    ],
    "require": {
	    "illuminate/support": "^9",
	    "cviebrock/eloquent-sluggable": "^9.0"
	},
	"require-dev": {
	    "phpunit/phpunit": "^9.0",
	    "orchestra/testbench": "^7.0"
	},
    "autoload": {
        "psr-4": {
            "AidynMakhataev\\LaravelSurveyJs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AidynMakhataev\\LaravelSurveyJs\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AidynMakhataev\\LaravelSurveyJs\\LaravelSurveyJsServiceProvider"
            ],
            "aliases": {
                "LaravelSurveyJs": "AidynMakhataev\\LaravelSurveyJs\\Facades\\LaravelSurveyJs"
            }
        }
    }
}
